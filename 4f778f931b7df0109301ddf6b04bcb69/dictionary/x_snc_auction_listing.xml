        <element default="0" label="Base price" max_length="40" name="base_price" type="integer"/>
        <element default="0" label="Current price" max_length="40" name="current_price" type="integer"/>
        <element label="Description" max_length="8000" name="description" type="html"/>
        <element label="Event" max_length="32" name="event" reference="x_snc_auction_event" type="reference"/>
        <element label="Highest bidder" max_length="32" name="highest_bidder" read_only="true" reference="sys_user" type="reference"/>
